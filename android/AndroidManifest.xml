<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="nl.nikhef.eduroam" android:installLocation="auto" android:versionCode="20140109" android:versionName="0.14">
    <uses-sdk android:minSdkVersion="14" android:targetSdkVersion="19"/>
    <uses-feature android:name="android.hardware.touchscreen" android:required="false"/>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
	<uses-permission android:name="android.permission.INTERNET" />
    <application android:allowBackup="false" android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:name="nl.nikhef.eduroam.WiFiEduroam"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    <activity android:name="WiFiACESettings" 
      android:label="WiFi Advanced Config Editor Settings">
    </activity>
</application>



</manifest> 